<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_07">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="7.5"
            android:orientation="vertical"
            android:weightSum="100">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="89">
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="7"
                android:orientation="horizontal"
                android:weightSum="100">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="40">
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_home"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="60"
                    android:orientation="horizontal">
                </LinearLayout>

            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="92"
            android:orientation="vertical"
            android:weightSum="100">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="40">
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="17">
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="16"
                android:orientation="horizontal"
                android:weightSum="100">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="12.4">
                </LinearLayout>
                <!--CONTROL-->
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="9"
                    android:orientation="horizontal"
                    android:weightSum="100">

                    <ImageView
                        android:id="@+id/iv_germ1_control"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ2_control"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>

                </RelativeLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10.5">
                </LinearLayout>
                <!--CARBOCISTEINE-->
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="8.8">


                    <ImageView
                        android:id="@+id/iv_germ1_carbocisteine"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ2_carbocisteine"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ3_carbocisteine"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ4_carbocisteine"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>


                </RelativeLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10.7">
                </LinearLayout>
                <!--ZINC-->
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="9">

                    <ImageView
                        android:id="@+id/iv_germ1_zinc"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ2_zinc"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ3_zinc"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ4_zinc"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ5_zinc"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ6_zinc"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>

                </RelativeLayout>
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10.6">
                </LinearLayout>
                <!--SOLMUX-->
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="8.9">


                    <ImageView
                        android:id="@+id/iv_germ1_solmux"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ2_solmux"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ3_solmux"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ4_solmux"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ5_solmux"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ6_solmux"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ7_solmux"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ8_solmux"
                        android:layout_width="7dp"
                        android:layout_height="7dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ9_solmux"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>
                    <ImageView
                        android:id="@+id/iv_germ10_solmux"
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/germ_2"/>

                </RelativeLayout>

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</RelativeLayout>